<%= form_with model:@user, url:"/users/#{@user.id}",local:true do |f| %>
  <h2>名前</h2>
  <%= f.text_field :name %>
  <h2>プロフィール画像</h2>
  <%= attachment_image_tag @user, :image,format:"jpeg",size: "100x100" %></br>
  <%= f.attachment_field :image %>
  <h2>紹介文</h2>
  <%= f.text_area :introduction %></br>
  <%= f.submit "保存" %>
<% end %>